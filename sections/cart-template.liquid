          <script>

                  function DTextBox_AddToIntValue_{{ item.id }}(targetId,addToValue)
                  {
                      var input = document.getElementById(targetId);
                      var textInt = parseInt(input.value);
                      if(isNaN(textInt))
                      {
                          textInt = 0;
                      }
                      
                    
                     var total_quantity= textInt + addToValue;
                   //input.value=total_quantity;
                    
                   if(total_quantity<=0){
                        input.value=0;
                    } else{
                       input.value=total_quantity;
                    }
                    
                   
                    
                  }

