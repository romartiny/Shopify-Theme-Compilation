           -->
            <div class="cart_quantity">
                  <!-- <a class="plus-minus" onclick="DTextBox_AddToIntValue_{{ item.id }}('updates_{{ item.id }}',-1)"><i class='iconsh cart_minus'></i></a> -->
                    <input name="updates[]" class="text_cart_box offerUpdate" data="{{item.product.id }}" id="updates_{{ item.id }}" type="number" value="{{ item.quantity }}" min="0" size="3" itemref="{{item.properties.upsellcheckquantity}}"/>
                   <!-- <a class="plus-minus" onclick="DTextBox_AddToIntValue_{{ item.id }}('updates_{{ item.id }}',+1)"><i class='iconsh cart_plus'></i></a> -->

