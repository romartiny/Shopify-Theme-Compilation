              <div class="td_price">
              <span class="cart__subtotal-title" >
                {{ 'cart.general.was' | t }}
              </span>
              <span class="cart__subtotal">{{ total_saving_was | money }}</span>        
              </div>
              {% endif %}
              {% endif %}
              
              {% if section.settings.saved_price  %}
