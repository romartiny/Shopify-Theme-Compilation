  .checkout_btn{
    background-color:{{ section.settings.checkout_bg_color }};
    font-size:{{ section.settings.checkout_button_font_size }}px !important;
    font-family:{{ section.settings.checkout_button_font_family }}!important;
  }
  .checkout_btn:hover{
    background-color:{{ section.settings.checkout_bg_hover_color }} !important;
  }
  
  .timer_box samp {
     color: {{ section.settings.cart_timer_color }} !important;
 }
  .cart__subtotal-title , .cart__subtotal  span{
    color:{{section.settings.price_color}} !important;
    font-size:{{section.settings.price_font}}px !important;
    font-family:{{section.settings.price_fontfamily}} !important;
  }
</style>
<div class="page-width" data-section-id="{{ section.id }}" data-section-type="cart-template">
    {% if cart.item_count > 0 %}
  <form action="/cart" method="post" id="update_cart" novalidate   class="cart small--hide desktopcartnew">
  

    <div class="section-header text-center">
       {% if section.settings.up_checkout_button_enable %}
   
    <input  type="submit" name="checkout" class="top_checkout checkout_btn btn" value="{{ 'cart.general.checkout' | t }}">
   {% endif %}
      <h1>{{ 'cart.general.title' | t }}</h1>
   
    </div>
    {% if section.settings.cart_enable %}
{{ 'jquery.countdownTimer_2hours.js' | asset_url | script_tag }}  
  
  {% endif %}
  
    <table class="cart-table full table--responsive">
   
      <thead class="cart__row cart__header-labels">
        <th colspan="2" class="text-center set-align crt">{{ 'cart.label.product' | t }}</th>
        <th class="text-center quantity_cart set-align">{{ 'cart.label.quantity' | t }}</th>
        <th class="text-left price_cart">{{ 'cart.label.price' | t }}</th>
        <th class="text-left total_cart">{{ 'cart.label.total' | t }}</th>       
      </thead>
  
      <tbody>
        {% for item in cart.items %}
        
          {% if item.variant.compare_at_price > item.variant.price %}
          {% assign saving = item.variant.compare_at_price | minus: item.variant.price | times: item.quantity %}
          {% assign was = item.variant.compare_at_price |  times: item.quantity %}
          {% assign total_saving = saving | plus: total_saving %}
          {% assign total_saving_was = was | plus: total_saving_was %}
        
          {% endif %} 
        
        
       
          <tr class="cart__row table__section">
            <td class="order_product_cart" data-label="{{ 'customer.order.product' | t }}">
              <a href="{{ item.url | within: collections.all }}" class="cart__image">
                {% comment %}
                  More image size options at:
                    - http://docs.shopify.com/themes/filters/product-img-url
                {% endcomment %}
                <img class="nav-bar desktop_nav" src="{{ item | img_url: 'small' }}" alt="{{ item.title | escape }}">
                <img class="nav-bar mobile_nav" src="{{ item | img_url: 'large' }}" alt="{{ item.title | escape }}">

              </a>
            </td>
            <td class="product_title_cart">
              <a href="{{ item.url }}" class="h5">
                {{ item.product.title }}
              </a>
              {% comment %}
              {% unless item.variant.title contains 'Default' %}
                <br>
                <small>{{ item.variant.title }}</small>
              {% endunless %}
              
              {% endcomment %}
              
              
                              {% unless item.variant.title contains 'Default' %}
                  <div class="cart__meta-text">
                    {% for option in item.product.options %}
                      {{ option }}: {{ item.variant.options[forloop.index0] }}<br/>
                    {% endfor %}
                  </div>
                {% endunless %}

              {% if section.settings.cart_vendor_enable %}
                <p>{{ item.vendor }}</p>
              {% endif %}

              {% comment %}
                Optional, loop through custom product line items if available

                For more info on line item properties, visit:
                  - http://docs.shopify.com/support/your-store/products/how-do-I-collect-additional-information-on-the-product-page-Like-for-a-monogram-engraving-or-customization
              
             
              {% assign property_size = item.properties | size %}
              {% if property_size > 0 %}
                {% for p in item.properties %}
                  {% unless p.last == blank %}
                    {{ p.first }}:

                    {% comment %}
                      Check if there was an uploaded file associated
                    {% endcomment %}
                    {% if p.last contains '/uploads/' %}
                      <a href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>
                    {% else %}
                      {{ p.last }}
                    {% endif %}

                    <br>
                  {% endunless %}
                {% endfor %}
              {% endif %}

              {% endcomment %}

              
              {% for free in item.properties %}
              {% assign freeitemParent = free.last  %}
              {% endfor %}
              
              
              
              <a data="{{ item.product.id }}" href="javascript:;"  class="cart__remove cardesktop">
                <small>{{ 'cart.general.remove' | t }}</small>
              </a>
              
            </td>
           
              {% assign  freeItem=all_products[settings.free_product].selected_or_first_available_variant.id %}

          
            {% if item.properties.free_product == 'true' %}
            
            
            <td class="quantity_cart" data-label="{{ 'cart.label.quantity' | t }}">
            <!--  <input type="number" readonly name="updates[]" class="text_cart_box free_{{ freeitemParent | first }}" id="updates_{{ item.id }}" value="{{ item.quantity }}" min='0'>
           -->
             <div class="cart_quantity not_click_btn">
                   <!-- <a class="plus-minus"><i class='iconsh cart_minus'></i></a>-->
                    <input name="updates[]" readonly class="text_cart_box free_{{ item.properties.free_product_parent_id }}" id="updates_{{ item.id }}" type="number" value="{{ item.quantity }}" min="0" size="3" />
                  <!-- <a class="plus-minus" ><i class='iconsh cart_plus'></i></a>-->

              </div>
            
            
            </td> 
            {% else %}
            <td class="quantity_cart" data-label="{{ 'cart.label.quantity' | t }}">
