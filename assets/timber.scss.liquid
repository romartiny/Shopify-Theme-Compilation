}

.site-header__logo {
  text-align: center;
  margin: 0 auto;
  max-width: 100%;



  a,
  a:hover,
  a:focus {
    text-decoration: none;
  }

  a, img {
    display: block;
  }

  img {
    margin: 0 auto;
  }
}

.site-header__logo-link {
  max-width: $logoMaxWidth;
  margin: 0 auto;
}

.site-header__cart-toggle {
  display: inline-block;
}

.site-header__search {

  margin-top: $gutter / 2;

}

.search-bar {
  max-width: 100%;

 
}

/*============================================================================
  #Site Nav and Dropdowns
==============================================================================*/
.nav-bar {
 /* background-color: $colorNav;*/
}

.site-nav {
  font-size: {{ settings.menu_font_size }}px;
  cursor: default;
  margin: 0 0 0 (-$gutter / 2);

  li {
    margin: 0;
    display: block;
  }

  > li {
    position: relative;
    display: inline-block;
  }
}

/*================ Home to hamburger and cart that toggle drawers ================*/
.site-nav--mobile {
  @extend .site-nav;

  .text-right & {
    margin: 0 (-$gutter / 2) 0 0;
  }
}
.desktop.site-header a, .desktop.site-header h3 {
 Font-family: {{ settings.menu_font_family }} !important;
}

.site-nav__link {
  display: block;
  text-decoration: none;
  padding: $gutter / 2;
  white-space: nowrap;
  color: {{ settings.menu_color_primary }};
  Font-family: {{ settings.menu_font_family }} ;
  &:hover,
  &:active,
  &:focus {
    color: darken($colorNavText, 15%);
        cursor: pointer;
  }

  .icon-arrow-down {
    position: relative;
    top: -2px;
    font-size: 10px;
    padding-left: $gutter / 4;
  }

  .site-nav--active > & {
    
  }

  .site-nav--mobile & {
    display: inline-block;
  }
